{
  "expo": {
    "name": "SIMKAS",
    "slug": "SIMKAS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/simkas-logo.png",
    "scheme": "simkas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.highfiveindonesia.SIMKAS"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icons/simkas-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/simkas-logo.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#fefefe",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "26d2cd69-2e5e-4407-b772-c09ae9f8a5d6"
      }
    },
    "owner": "jieprass"
  }
}
